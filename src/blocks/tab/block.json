{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "wpinc/tab",
	"version": "0.1.0",
	"title": "Tab",
	"category": "design",
	"icon": "category",
	"description": "By clicking on the tabs that appear, you can switch between the contents. There are two ways to display tabs: 'scroll' and 'stack'.",
	"supports": {
		"html": false
	},
	"attributes": {
		"type": {
			"type": "string",
			"default": "scroll"
		}
	},
	"example": {
		"attributes": {
			"type": "scroll"
		}
	},
	"textdomain": "wpinc_blok",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}